# Mind-Bridge AI Analysis Configuration
# European Compliance Edition

# OpenAI Configuration (obrigatório para análise de IA)
OPENAI_API_KEY=sua_chave_openai_aqui
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.3
OPENAI_MAX_TOKENS=2000

# Configurações de Compliance
GDPR_COMPLIANCE=true
AI_ACT_COMPLIANCE=true
PROFESSIONAL_OVERSIGHT_REQUIRED=true
DATA_RETENTION_DAYS=365

# Configurações do Sistema
FLASK_ENV=development
SECRET_KEY=mindbridge_eu_compliance_2024
DATABASE_URL=sqlite:///mindbridge_eu.db

# Configurações de Performance
AI_ANALYSIS_TIMEOUT=30
MAX_BATCH_SIZE=50
CONFIDENCE_THRESHOLD=0.7

# APIs Governamentais Brasileiras

# SUS (Sistema Único de Saúde)
SUS_API_URL=https://apidadosabertos.saude.gov.br
SUS_API_KEY=your_sus_api_key

# INSS (Instituto Nacional do Seguro Social)
INSS_API_URL=https://api.inss.gov.br
INSS_API_KEY=your_inss_api_key

# SIAFI (Sistema Integrado de Administração Financeira)
SIAFI_API_URL=https://api.siafi.tesouro.gov.br
SIAFI_API_KEY=your_siafi_api_key

# Portal da Transparência
PORTAL_TRANSPARENCIA_URL=https://api.portaltransparencia.gov.br/api-de-dados
PORTAL_TRANSPARENCIA_TOKEN=your_portal_token

# RAPS (Rede de Atenção Psicossocial)
RAPS_API_URL=https://api.raps.saude.gov.br
RAPS_API_KEY=your_raps_api_key

# CNES (Cadastro Nacional de Estabelecimentos de Saúde)
CNES_API_URL=https://cnes.datasus.gov.br/api
CNES_API_KEY=your_cnes_api_key

# E-mail e Notificações
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@domain.com
SMTP_PASS=your_app_password

# Configurações de Rate Limiting para APIs Governamentais
GOV_API_RATE_LIMIT_PER_MINUTE=60
GOV_API_TIMEOUT=15